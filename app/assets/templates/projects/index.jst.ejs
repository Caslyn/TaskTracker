
<div class="projectsIndex col-md-5">
	<h3>Projects</h3>
	<div class="project-list">
		<% collection.each( function(project) { %>
			<div class="panel panel-default"
				<div class="project-board panel-body">
					<a href="#/projects/<%= project.id %>"><%= project.escape('title') %></a>
				</div>
			</div>
		<% }) %>
	</div>
</div>