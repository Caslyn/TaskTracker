<div class="panel panel-default">
	<div class="panel-heading">
		<h3>Projects</h3>
	</div>
	<table class="table">
		<tr>
			<th>Project</th>
		</tr>
		<% projects.each(function(project) { %>
			<tr>
				<td>
					<a href="#/projects/<%= project.id %>"><%= project.escape('title') %></a>
				</td>
			</tr>
		<% }) %>
	</table>
</div
